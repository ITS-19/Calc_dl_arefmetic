        public static string Delenie(string st1, string st2)//деление(надо доработать)
        {
            BigInteger s1 = BigInteger.Parse(st1);
            BigInteger s2 = BigInteger.Parse(st2);
            string result;
            BigInteger r = s1 / s2;
            BigInteger o = s1 % s2;
            if (o == 0)
            {
                result = r + "";
            }
            else
            {
                result = r + ",";
            }


            return result;
        }
