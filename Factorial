        public static string Fact(string st1)//нахождение факториала
        {
            BigInteger s1 = BigInteger.Parse(st1);
            BigInteger p;
            p = 1;
            for (int i = 1; i < s1; i++)
            {
                p += p * i;
            }
            string result = p + "";
            return result;
        }
